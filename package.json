{
  "name": "@kloudless/file-explorer",
  "version": "1.9.0",
  "author": "Kloudless <hello@kloudless.com> (https://kloudless.com)",
  "description": "The Kloudless File Explorer is a JavaScript library that allows your users to browse and select files and folders from their storage services. It comes in two modes: a chooser and a saver.",
  "main": "./dist/loader/js/interface.js",
  "types": "./dist/loader/js/interface.d.ts",
  "keywords": [
    "kloudless",
    "file picker",
    "file explorer",
    "cloud storage"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/kloudless/file-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/kloudless/file-explorer/issues",
    "email": "support@kloudless.com"
  },
  "homepage": "https://kloudless.com/products/file-explorer",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development node ./dev-server/dev-server.js",
    "install-deps": "bower install",
    "transpile": "BABEL_ENV='transpile' babel src/loader/js -d dist/loader/js --ignore src/loader/js/webpack --copy-files",
    "storybook:vue": "npm run storybook --prefix storybook-vue/",
    "storybook:react": "npm run storybook --prefix storybook-react",
    "build:storybook": "npm run build --prefix ./storybook-vue && npm run build --prefix ./storybook-react",
    "deploy:storybook": "npm run build:storybook && storybook-to-ghpages --remote=github --existing-output-dir=.demo",
    "build": "rm -rf dist/loader dist/explorer test/dist && NODE_ENV=production webpack --config config/webpack.prod.conf.js",
    "build:template": "pug src/explorer/templates/explorer.pug -o dist/template -s && ejs-cli template/explorer.ejs > dist/custom-explorer.html && rm -rf dist/template",
    "prepack": "bash generate_npmignore.sh && npm install && npm run build && npm run transpile",
    "postpack": "rm -f .npmignore",
    "dist-test": "node ./test/dist-test-server.js",
    "test:ts": "eslint test/ts/*.ts && tsc test/ts/*.ts --noEmit && echo 'âœ…  Passed type tests.'"
  },
  "dependencies": {
    "hoist-non-react-statics": "3.3.0",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "@babel/preset-env": "7.4.1",
    "@babel/preset-react": "7.0.0",
    "@storybook/storybook-deployer": "2.8.1",
    "@typescript-eslint/eslint-plugin": "2.9.0",
    "@typescript-eslint/parser": "2.9.0",
    "autoprefixer": "9.6.1",
    "autoprefixer-stylus": "0.14.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-define": "1.3.1",
    "bower": "1.8.8",
    "cldr-data-downloader": "0.3.5",
    "copy-webpack-plugin": "5.0.3",
    "core-js": "3.1.4",
    "css-loader": "3.0.0",
    "cssnano": "4.1.10",
    "csso-stylus": "0.0.3",
    "ejs": "2.6.2",
    "ejs-cli": "2.0.1",
    "eslint": "5.15.3",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "expose-loader": "0.7.5",
    "express": "4.17.1",
    "html-webpack-plugin": "3.2.0",
    "jade": "1.3.1",
    "jquery": "2.1.1",
    "knockout": "^3.5.0",
    "mini-css-extract-plugin": "0.7.0",
    "moment": "^2.24.0",
    "morgan": "1.9.1",
    "postcss-loader": "3.0.0",
    "pug": "2.0.4",
    "pug-cli": "1.0.0-alpha6",
    "pug-loader": "2.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "regenerator-runtime": "0.13.2",
    "sammy": "0.7.6",
    "script-ext-html-webpack-plugin": "2.1.3",
    "stylus": "0.48.1",
    "stylus-loader": "3.0.2",
    "typescript": "3.7.2",
    "url-loader": "2.0.1",
    "webpack": "^4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-middleware": "3.7.0",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "4.2.1"
  }
}
